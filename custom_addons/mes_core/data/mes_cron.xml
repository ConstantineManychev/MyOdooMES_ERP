<odoo>
    <data noupdate="1">
        <record id="ir_cron_sync_maintainx_tasks" model="ir.cron">
            <field name="name">MES: Tasks Loading from MaintainX</field>
            <field name="model_id" ref="model_mes_task"/>
            <field name="state">code</field>
            <field name="code">model.load_tasks_from_maintainx()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/> 
        </record>
    </data>
</odoo>